go.property('speed', vmath.vector4(0.3, 0, 0, 0))
go.property('scale', vmath.vector4(0.25, 0, 0, 0))

function init(self)
	self.time = 0
	model.set_constant("#model", "speed", self.speed)
	model.set_constant("#model", "scale", self.scale)
end

function update(self, dt)
	self.time = self.time + dt
	model.set_constant("#model", "iTime", vmath.vector4(self.time, 0,0,0))
end